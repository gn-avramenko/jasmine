<?xml version="1.0" encoding="UTF-8"?>
<rest xmlns="http://gridnine.com/jasmine/rest" id="standard">
    <entity id="com.gridnine.jasmine.common.standard.model.rest.Select2QueryParameter">
        <property id="key" type="STRING"/>
        <property id="value" type="STRING"/>
    </entity>
    <group id="standard">
        <operation id="autocompleteSelect2" handler="com.gridnine.jasmine.server.standard.rest.Select2AutocompleteRestHandler">
            <request id="com.gridnine.jasmine.common.standard.model.rest.Select2AutocompleteRequest">
                <property id="limit" type="INT" non-nullable="true"/>
                <property id="searchText" type="STRING"/>
                <collection id="queryParameters" element-type="ENTITY" element-class-name="com.gridnine.jasmine.common.standard.model.rest.Select2QueryParameter"/>
            </request>
            <response id="com.gridnine.jasmine.common.standard.model.rest.Select2AutocompleteResponse">
                <collection id="values" element-type="ENTITY" element-class-name="com.gridnine.jasmine.common.core.model.SelectItem"/>
            </response>
        </operation>
    </group>
</rest>



