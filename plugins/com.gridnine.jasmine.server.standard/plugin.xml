<plugin xmlns="http://xtrip.gridnine.com/spf" id="com.gridnine.jasmine.server.standard">
    <parameter id="type" value="SERVER"/>
    <dependencies>
        <plugin-dependency plugin-id="com.gridnine.jasmine.server.core"/>
    </dependencies>
    <extension point-id="rest-metadata">
        <parameter id="relative-path" value="source/com/gridnine/jasmine/server/standard/model/standard-rest.xml"/>
        <parameter id="url" value="com/gridnine/jasmine/server/standard/model/standard-rest.xml"/>
        <parameter id="dest-server-plugin-id" value="com.gridnine.jasmine.server.standard"/>
        <parameter id="dest-client-plugin-id" value="com.gridnine.jasmine.web.core"/>
    </extension>
    <extension point-id="domain-metadata">
        <parameter id="relative-path" value="source/com/gridnine/jasmine/server/standard/model/standard-domain.xml"/>
        <parameter id="url" value="com/gridnine/jasmine/server/standard/model/standard-domain.xml"/>
        <parameter id="dest-server-plugin-id" value="com.gridnine.jasmine.server.standard"/>
        <parameter id="dest-client-plugin-id" value="com.gridnine.jasmine.web.core"/>
    </extension>
    <extension point-id="ui-metadata">
        <parameter id="relative-path" value="source/com/gridnine/jasmine/server/standard/model/standard-ui.xml"/>
        <parameter id="url" value="com/gridnine/jasmine/server/standard/model/standard-ui.xml"/>
        <parameter id="dest-server-plugin-id" value="com.gridnine.jasmine.server.standard"/>
        <parameter id="dest-client-plugin-id" value="com.gridnine.jasmine.web.core"/>
    </extension>
    <extension point-id="web-messages">
        <parameter id="relative-path" value="source/com/gridnine/jasmine/server/standard/model/standard-web-messages.xml"/>
        <parameter id="url" value="com/gridnine/jasmine/server/standard/model/standard-web-messages.xml"/>
        <parameter id="dest-client-plugin-id" value="com.gridnine.jasmine.web.core"/>
    </extension>
    <extension point-id="activator">
        <parameter id="class" value="com.gridnine.jasmine.server.standard.activator.StandardServerActivator"/>
    </extension>
    <extension point-id="server-messages">
        <parameter id="relative-path" value="source/com/gridnine/jasmine/server/standard/model/standard-server-messages.xml"/>
        <parameter id="url" value="com/gridnine/jasmine/server/standard/model/standard-server-messages.xml"/>
        <parameter id="dest-server-plugin-id" value="com.gridnine.jasmine.server.standard"/>
    </extension>
</plugin>
